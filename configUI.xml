<?xml version='1.0' encoding="utf-8"?>
<config_root>
	<show_title>File Manager</show_title>
	<mainFormBackColor>LightCyan</mainFormBackColor>
	<volume_select_label>磁盘选择：</volume_select_label>
	<volumeLabelBackColor>LimeGreen</volumeLabelBackColor>
	<volumeSelectBackColor>LimeGreen</volumeSelectBackColor>
	<reload_volume_label>重新载入</reload_volume_label>
	<volumeloadingButtonBackColor>SpringGreen</volumeloadingButtonBackColor>
	<data_table tableName="showAllFiles" tableBackgroundColor="Lavender" columnHeadersBackColor="LightSteelBlue"
		rowsBackColor="Snow" rowMouseMoveBackColor="Lime" rowMouseLeaveBackColor="Beige">
		<column name="name" data_type="text" width="180"/>
		<column name="size" data_type="long" width="100"/>
		<column name="path" data_type="text" width="300"/>
		<column name="createDate" data_type="text" width="120"/>
		<column name="modifyDate" data_type="text" width="120"/>
		<column name="lastAccessDate" data_type="text" width="120"/>
	</data_table>
	<data_table tableName="showSameFiles" tableBackgroundColor="Lavender" columnHeadersBackColor="LightSteelBlue"
		rowsBackColor="Snow" rowMouseMoveBackColor="Lime" rowMouseLeaveBackColor="Beige">
		<column name="name" data_type="text" width="250"/>
		<column name="times" data_type="long" width="50"/>
		<column name="size" data_type="long" width="100"/>
		<column name="path" data_type="text" width="600"/>
	</data_table>
</config_root>
